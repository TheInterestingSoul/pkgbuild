pkgname=pantum-driver
pkgver="1.1.74"
pkgrel=1
pkgdesc="CUPS and SANE drivers for Pantum series printer and scanner"
arch=('x86_64')
url="http://global.pantum.com/global/"
license=('proprietary')
depends=('cups')
source=(
  "pantum_1.1.74-1_amd64.deb" # extract from .zip manually
  "pantum-driver.install"
)
md5sums=('SKIP'
         'SKIP')

install=pantum-driver.install

package() {
  tar -xf "$srcdir/data.tar.xz" -C "$pkgdir/"
}

